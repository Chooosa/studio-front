  useEffect(() => {\n\n    localStorage.setItem('scroll', false)\n  }, [])\n\n\n  useEffect(() => {\n    dispatch(fetchContent())\n  }, [dispatch])\n\n  const { ErrorBoundary, didCatch, error } = useErrorBoundary();\n\n  return (\n    <AppWrapper>\n      {\n        didCatch ? <ErrorFallback error={error} /> :\n          <ErrorBoundary>\n            <MenuState>\n              <Header />\n            </MenuState>\n\n            <AppContainer>\n              {/* <AnimatePresence > */}\n              <Switch location={location} key={location.pathname}>\n                <Redirect from=\"/:url*(/+)\" to={pathname.slice(0, -1)} />\n                <Route exact path='/' component={Main} />\n                <Route exact path='/works/:section/:itemId' component={WorksPage} />\n                {/* <Route exact path='/works/:section/:itemId' component={WorksPage} /> */}\n                <Route exact path='/services/:section' component={ServicesPage} />\n                <Route exact path='/cooperation' component={Cooperation} />\n                <Route exact path='/guarantees' component={Guarantees} />\n                <Route exact path={`/contacts`} component={Contacts} />\n                <Route path='/' >\n                  <NotFound endLoader={endLoader} error={error} />\n                </Route>\n              </Switch>\n              {/* </AnimatePresence> */}\n              {/* <button onClick={() => setAddr()}>change</button> */}\n            </AppContainer>\n            <AnimatedBackground />\n            <Footer />\n          </ErrorBoundary>\n      }\n      <Loader setEndLoader={setEndLoader} />\n    </AppWrapper>\n  );\n}\n\nexport default App;","/home/roman/Documents/Work/lilekov-studio-front/src/redux/store.jsx",[],"/home/roman/Documents/Work/lilekov-studio-front/src/App.styles.jsx",[],"/home/roman/Documents/Work/lilekov-studio-front/src/context/menu-state.jsx",[],"/home/roman/Documents/Work/lilekov-studio-front/src/redux/rootReducer.jsx",[],"/home/roman/Documents/