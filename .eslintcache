s'))\n                    history.push('/')\n                }\n                break;\n\n            case 'plan':\n                if (pathname === '/') {\n                    dispatch(setScroll('plan'))\n                } else {\n                    dispatch(setScroll('plan'))\n                    history.push('/')\n                }\n                break;\n\n            case 'applicationAll':\n                history.push('/works/Application/all')\n                break;\n\n            case 'websiteAll':\n                history.push('/works/Website/all')\n                break;\n\n            case 'application':\n                history.push('/services/Application')\n                break;\n\n            case 'website':\n                history.push('/services/Website')\n                break;\n\n            case 'service':\n                history.push('/services/Service')\n                break;\n\n            case 'guarantees':\n                history.push('/guarantees')\n                break;\n\n            case 'cooperation':\n                history.push('/cooperation')\n                break;\n\n            default:\n                break;\n        }\n\n        toggleMenuMode()\n    }\n\n    return (\n        <AnimatePresence>\n            {\n                isMenuOpen ?\n                    <Container\n                        variants={variants}\n                        exit='hidden'\n                        initial='hidden'\n                        animate='shown'\n                    >\n                        <MenuSection\n                            title={'Меню'}\n                        >\n                            <BodyItem>\n                                <span to='/' onClick={() => handleNavigation('typing')}>\n                                    Главная\n                                </span>\n                            </BodyItem>\n                            <BodyItem\n                            >\n                                <span onClick={() => handleNavigation('services')}>\n                                    Услуги\n                                </span>\n                            </BodyItem>\n                            <BodyItem>\n                                <span onClick={() => handleNavigation('cases')}>\n                                    Кейсы\n                                </span>\n                            </BodyItem>\n                            <BodyItem>\n                                <span onClick={() => handleNavigation('plan')}>\n                                    План работы\n                                </span>\n                            </BodyItem>\n                        </MenuSection>\n                        <MenuSection\n                            title={'Портфолио'}\n                        >\n                            <BodyItem\n                            >\n                                <span onClick={() => handleNavigation('applicationAll')}>\n                                    Приложения\n                                </span>\n                            </BodyItem>\n                            <BodyItem>\n                                <span onClick={() => handleNavigation('websiteAll')}>\n                                    Сайты\n                                </span>\n                            </BodyItem>\n                        </MenuSection>\n                        <MenuSection\n                            title={'Услуги'}\n                        >\n                            <BodyItem\n                            >\n                                <span onClick={() => handleNavigation('application')}>\n                                    Приложения\n                                </span>\n                            </BodyItem>\n                            <BodyItem>\n                                <span onClick={() => handleNavigation('website')}>\n                                    Сайты\n                                </span>\n                            </BodyItem>\n                            <BodyItem>\n                                <span onClick={() => handleNavigation('service')}>\n                                    Доп. услуги\n                                </span>\n                            </BodyItem>\n                        </MenuSection>\n                        <MenuSection\n                            title={'Разное'}\n                        >\n                            <BodyItem>\n                                <span onClick={() => handleNavigation('guarantees')}>\n                                    Гарантии\n                                </span>\n                            </BodyItem>\n                            <BodyItem>\n                                <span onClick={() => handleNavigation('cooperation')}>\n                                    Сотрудничество\n                                </span>\n                            </BodyItem>\n                        </MenuSection>\n                        <Divider\n                            color={color}\n                            initial={{\n                                width: '0%'\n                            }}\n                            animate={{\n                                width: '100%',\n                                transition: {\n                                    duration: 1.5\n                                }\n                            }}\n                        />\n                        <MenuSection\n                            title={'Контакты'}\n                            color={color}\n                            keepOpen={true}\n                        >\n                            <BodyItem>\n                                <a href='tel:+79995357879'>\n                                    8 999 535 78 79\n                                </a>\n                            </BodyItem>\n\n                            <BodyItem>\n                          