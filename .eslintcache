e={{\n                            position: 'fixed',\n                            width: '100%',\n                            height: '100%',\n                            display: 'flex',\n                            justifyContent: 'center',\n                            alignItems: 'center',\n                            top: 0,\n                            left: 0,\n                            backgroundColor: 'rgba(0,0,0,0.2)',\n                            zIndex: 1110\n                        }}\n                        onClick={reverseFullScreen}\n                    >\n                    </div>\n                    : null\n            }\n        </Container>\n    )\n}\n\n\nexport default WorkItem;","/Users/admin/Desktop/WORK/lilekov-studio-front/src/components/Common/Section/section.styles.jsx",[],"/Users/admin/Desktop/WORK/lilekov-studio-front/src/components/Common/SectionHeader/section-header.component.jsx",[],"/Users/admin/Desktop/WORK/lilekov-studio-front/src/components/Common/AnimatedNumbers/animated-numbers.styles.jsx",[],"/Users/admin/Desktop/WORK/lilekov-studio-front/src/redux/color/color.actions.jsx",[],"/Users/admin/Desktop/WORK/lilekov-studio-front/src/components/Logo/logo.styles.jsx",[],"/Users/admin/Desktop/WORK/lilekov-studio-front/src/components/Common/AnimatedNumbers/Numbers/07.jsx",[],"/Users/admin/Desktop/WORK/lilekov-studio-front/src/components/Common/AnimatedNumbers/Numbers/06.jsx",[],"/Users/admin/Desktop/WORK/lilekov-studio-front/src/components/Common/AnimatedNumbers/Numbers/04.jsx",[],"/Users/admin/Desktop/WORK/lilekov-studio-front/src/components/Common/AnimatedNumbers/Numbers/03.jsx",[],"/Users/admin/Desktop/WORK/lilekov-studio-front/src/components/Common/AnimatedNumbers/Numbers/02.jsx",[],"/Users/admin/Desktop/WORK/lilekov-studio-front/src/components/Common/AnimatedNumbers/Numbers/05.jsx",[],"/Users/admin/Desktop/WORK/lilekov-studio-front/src/components/Map/custom-map.styles.jsx",[],"/Users/admin/Desktop/WORK/lilekov-studio-front/src/c