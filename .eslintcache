.push('/works/Website/all')\n                break;\n\n            case 'application':\n                history.push('/services/Application')\n                break;\n\n            case 'website':\n                history.push('/services/Website')\n                break;\n\n            case 'service':\n                history.push('/services/Service')\n                break;\n\n            case 'guarantees':\n                history.push('/guarantees')\n                break;\n\n            case 'cooperation':\n                history.push('/cooperation')\n                break;\n\n            default:\n                break;\n        }\n\n        toggleMenuMode()\n    }\n\n    return (\n        <AnimatePresence>\n            {\n                isMenuOpen ?\n                    <Container\n                        variants={variants}\n                        exit='hidden'\n                        initial='hidden'\n                        animate='shown'\n                    >\n                        <MenuSection\n                            title={'Меню'}\n                        >\n                            <BodyItem>\n                                <span to='/' onClick={() => handleNavigation('typing')}>\n                                    Главная\n                                </span>\n                            </BodyItem>\n                            <BodyItem\n                            >\n                                <span onClick={() => handleNavigation('services')}>\n                                    Услуги\n                                </span>\n                            </BodyItem>\n                            <BodyItem>\n                                <span onClick={() => handleNavigation('cases')}>\n                                    Кейсы\n                                </span>\n                            </BodyItem>\n                            <BodyItem>\n                                <span onClick={() => handleNavigation('plan')}>\n                                    План работы\n                                </span>\n                            </BodyItem>\n                        </MenuSection>\n                        <MenuSection\n                            title={'Портфолио'}\n                        >\n                            <BodyItem\n                            >\n                                <span onClick={() => handleNavigation('applicationAll')}>\n                                    Приложения\n                                </span>\n                            </BodyItem>\n                            <BodyItem>\n                                <span onClick={() => handleNavigation('websiteAll')}>\n                                    Сайты\n                                </span>\n                            </BodyItem>\n                        </MenuSection>\n                        <MenuSection\n                            title={'Услуги'}\n                        >\n                            <BodyItem\n                            >\n                                <span onClick={() => handleNavigation('application')}>\n                                    Приложения\n                                </span>\n                            </BodyItem>\n                            <BodyItem>\n                                <span onClick={() => handleNavigation('website')}>\n                                    Сайты\n                                </span>\n                            </BodyItem>\n                            <BodyItem>\n                                <span onClick={() => handleNavigation('service')}>\n                                    Доп. услуги\n                                </span>\n                            </BodyItem>\n                        </MenuSection>\n                        <MenuSection\n                            title={'Разное'}\n                        >\n                            <BodyItem>\n                                