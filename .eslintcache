{ x: '0%' }}\n         exit={{ x: '100%' }}\n         transition={{ duration: 0.3 }}\n\n      >\n         <Heading>\n            {t('analysis')}\n         </Heading>\n         <Text>\n            {t('analysis_desc_intro')}\n            <ul>\n               <li>\n                  {t('analysis_desc_list_first')}\n               </li>\n               <li>\n                  {t('analysis_desc_list_second')}\n               </li>\n               <li>\n                  {t('analysis_desc_list_third')}\n               </li>\n            </ul>\n            {t('analysis_desc_outro')}\n         </Text>\n      </motion.div>,\n\n      <motion.div\n         key={3}\n         initial={{ x: '-100%' }}\n         animate={{ x: '0%' }}\n         exit={{ x: '100%' }}\n         transition={{ duration: 0.3 }}\n\n      >\n         <Heading>\n            {t('briefing')}\n         </Heading>\n         <Text>\n            {t('briefing_desc_intro')}\n            <ul>\n               <li>\n                  {t('briefing_desc_list_first')}\n               </li>\n               <li>\n                  {t('briefing_desc_second')}\n               </li>\n            </ul>\n            {t('briefing_desc_outro')}\n         </Text>\n      </motion.div>,\n\n      <motion.div\n         key={4}\n         initial={{ x: '-100%' }}\n         animate={{ x: '0%' }}\n         exit={{ x: '100%' }}\n         transition={{ duration: 0.3 }}\n\n      >\n         <Heading>\n            {t('budget')}\n         </Heading>\n         <Text>\n            <p> {t('budget_desc')} </p>\n            <br />\n            <p> {t('budget_desc_second')} </p>\n         </Text>\n      </motion.div>,\n\n      <motion.div\n         key={5}\n         initial={{ x: '-100%' }}\n         animate={{ x: '0%' }}\n         exit={{ x: '100%' }}\n         transition={{ duration: 0.3 }}\n\n      >\n         <Heading>\n            {t('contract')}\n         </Heading>\n         <Text>\n            <p> {t('contract_desc')} </p>\n            <br /