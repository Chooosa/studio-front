if (scroll === 'plan') {\n         refSheme.current?.scrollIntoView({\n            behavior: 'smooth',\n            block: 'center'\n         })\n         dispatch(setScroll(undefined))\n      }\n   }, [scroll, dispatch])\n\n   const handleScroll = () => {\n      // refApplication.current.scrollIntoView({ behavior: 'smooth' })\n      dispatch(setScroll('request'))\n   }\n\n   const descriptionTexts = [\n      <motion.div\n         key={1}\n         initial={{ x: '-100%' }}\n         animate={{ x: '0%' }}\n         exit={{ x: '100%' }}\n         transition={{ duration: 0.3 }}\n\n      >\n         <Heading>\n            {t('interview')}\n         </Heading>\n         <Text>\n            {t('interview_desc_intro')}\n            <ul>\n               <li>\n                  {t('interview_desc_list_first')}\n               </li>\n               <li>\n                  {t('interview_desc_list_second')}\n               </li>\n               <li>\n                  {t('interview_desc_list_third')}\n               </li>\n            </ul>\n            {t('interview_desc_outro')}\n         </Text>\n      </motion.div>,\n\n      <motion.div\n         key={2}\n         initial={{ x: '-100%' }}\n         animate={{ x: '0%' }}\n         exit={{ x: '100%' }}\n         transition={{ duration: 0.3 }}\n\n      >\n         <Heading>\n            {t('analysis')}\n         </Heading>\n         <Text>\n            {t('analysis_desc_intro')}\n            <ul>\n               <li>\n                  {t('analysis_desc_list_first')}\n               </li>\n               <li>\n                  {t('analysis_desc_list_second')}\n               </li>\n               <li>\n                  {t('analysis_desc_list_third')}\n               </li>\n            </ul>\n            {t('analysis_desc_outro')}\n         </Text>\n      </motion.div>,\n\n      <motion.div\n         key={3}\n         initial={{ x: '-100%' }}\n         animate={{ x: '0%' }}\n         exit={{ x: '100%' }}\n         transition={{ duration: 0.3 }}\n\n      >\n         <Heading>\n            {t('briefing')}\n         </Heading>\n         <Text>\n            {t('briefing_desc_intro')}\n            <ul>\n               <li>\n                  {t('briefing_desc_list_first')}\n               </li>\n               <li>\n                  {t('briefing_desc_second')}\n               </li>\n            </ul>\n            {t('briefing_desc_outro')}\n         </Text>\n      </motion.div>,\n\n      <motion.div\n         key={4}\n         initial={{ x: '-100%' }}\n         animate={{ x: '0%' }}\n         exit={{ x: '100%' }}\n         transition={{ duration: 0.3 }}\n\n      >\n         <Heading>\n            {t('budget')}\n         </Heading>\n         <Text>\n            <p> {t('budget_desc')} </p>\n            <br />\n            <p> {t('budget_desc_second')} </p>\n         </Text>\n      </motion.div>,\n\n      <motion.div\n         key={5}\n         initial={{ x: '-100%' }}\n         animate={{ x: '0%' }}\n         exit={{ x: '100%' }}\n         transition={{ duration: 0.3 }}\n\n      >\n         <Heading>\n            {t('contract')}\n         </Heading>\n         <Text>\n            <p> {t('contract_desc')} </p>\n            <br />\n            <p> {t('contract_desc_second')} </p>\n            <br />\n            <p> {t('contract_desc_third')} </p>\n            <br />\n         </Text>\n      </motion.div>,\n\n      <motion.div\n         key={6}\n         initial={{ x: '-100%' }}\n         animate={{ x: '0%' }}\n         exit={{ x: '100%' }}\n         transition={{ duration: 0.3 }}\n\n      >\n         <Heading>\n            {t('design_specification')}\n         </Heading>\n         <Text>\n            <p> {t('design_specification_desc')} </p>\n            <br />\n            <p> {t('design_specification_desc_second')} </p>\n         </Text>\n      </motion.div>,\n\n      <motion.div\n         key={7}\n         initial={{ x: '-100%' }}\n         animate={{ x: '0%' }}\n         exit={{ x: '100%' }}\n         transition={{ duration: 0.3 }}\n\n      >\n         <Heading>\n            {t('design')}\n         </Heading>\n         <Text>\n            <p> {t('design_desc')} </p>\n            <br />\n            <p>{t('design_desc_second')}</p>\n            <br />\n            <p>{t('design_desc_third')}</p>\n         </Text>\n      </motion.div>,\n\n      <motion.div\n         key={8}\n         initial={{ x: '-100%' }}\n         animate={{ x: '0%' }}\n         exit={{ x: '100%' }}\n         transition={{ duration: 0.3 }}\n\n      >\n         <Heading>\n            {t('development')}\n         </Heading>\n         <Text>\n            <p> {t('development_desc')} </p>\n            <br />\n            <p> {t('development_desc_second')} </p>\n         </Text>\n      </motion.div>,\n\n      <motion.div\n         key={9}\n         initial={{ x: '-100%' }}\n         animate={{ x: '0%' }}\n         exit={{ x: '100%' }}\n         transition={{ duration: 0.3 }}\n\n      >\n         <Heading>\n            {t('testing')}\n         </Heading>\n         <Text>\n            <p> {t('testing_desc')} </p>\n            <br />\n            <p> {t('testing_desc_second')} </p>\n         </Text>\n      </motion.div>,\n\n      <motion.div\n         key={10}\n         initial={{ x: '-100%' }}\n         animate={{ x: '0%' }}\n         exit={{ x: '100%' }}\n         transition={{ duration: 0.3 }}\n\n      >\n         <Heading>\n            {t('support')}\n         </Heading>\n         <Text>\n            <p> {t('support_desc')} </p>\n            <br />\n            <p> {t('support_desc_second')} </p>\n         </Text>\n      </motion.div>,\n\n   ]\n\n   const handlePaintingOver = () => {\n      if (transitionProgress === 1) {\n         switch (currentStep) {\n            case 1:\n               return '0%'\n            case 2:\n               return '34%'\n            case 3:\n               return '64%'\n            case 4:\n               return '89%'\n            case 5:\n               return '100%'\n            default:\n               return '100%'\n         }\n      } else if (transitionProgress > 1) {\n         return '100%'\n      } else {\n         return '0%'\n      }\n   }\n\n   const handlePaintingOver2 = () => {\n      if (transitionProgress === 2) {\n         if (currentStep > 4) {\n            switch (currentStep) {\n               case 5:\n                  return '6%'\n               case 6:\n                  return '38%'\n               case 7:\n                  return '60%'\n               case 8:\n                  return '89%'\n               case 9:\n                  return '100%'\n               default:\n                  return '100%'\n            }\n         } else {\n            return '0%'\n         }\n      } else if (transitionProgress > 2) {\n         return '100%'\n      } else {\n         return '0%'\n      }\n   }\n\n   const handlePaintingOver3 = () => {\n      if (transitionProgress === 3) {\n         if (currentStep > 8) {\n            switch (currentStep) {\n               case 9:\n                  return '35%'\n               case 10:\n                  return '100%'\n               default:\n                  return '100%'\n            }\n         } else {\n            return '0%'\n         }\n      } else {\n         return '0%'\n      }\n   }\n\n   const handleTransitionEnd = () => {\n      if (line1.current?.offsetWidth === progressLine1.current?.offsetWidth) {\n         setTransitionProgress(2)\n      }\n   }\n\n   const handleTransitionEnd2 = () => {\n      if (progressLine1.current?.offsetWidth !== line1.current?.offsetWidth) {\n         setTransitionProgress(1)\n      }\n      if (line2.current?.offsetWidth === progressLine2.current?.offsetWidth) {\n         setTransitionProgress(3)\n      }\n      if (progressLine2.current?.offsetWidth === 0) {\n         setTransitionProgress(1)\n      }\n   }\n\n   const handleTransitionEnd3 = () => {\n      if (transitionProgress > 1) {\n         if (progressLine3.current?.offsetWidth === 0) {\n            setTransitionProgress(2)\n         }\n      }\n   }\n\n   return (\n      <PlanWrapper>\n         <PlanContainer>\n            <AnimateSharedLayout>\n               <DescriptionWrapper\n                  layout\n               >\n                  <AnimatePresence\n                     exitBeforeEnter\n                  >\n                     {descriptionTexts[currentStep - 1]}\n                  </AnimatePresence>\n               </DescriptionWrapper>\n            </AnimateSharedLayout>\n\n            <SchemeWrapper ref={refSheme}>\n               <LineWrapper>\n                  <Line\n                     ref={line1}\n                     lineWidth='calc(100% - 6%)'>\n                     <LineLabel\n                        left={'0%'}\n                     >\n                        {t('first_stage')}\n                     </LineLabel>\n                     <ProgressLine\n                        ref={progressLine1}\n                        cusstomWidth={handlePaintingOver}\n                        color={color}\n                        onTransitionEnd={handleTransitionEnd}\n                     />\n                     <ProgressDot\n                        left={handlePaintingOver}\n                        color={color}\n                        currentStep={currentStep}\n                        first\n                        transitionProgress={transitionProgress}\n                     />\n\n                     <Dash\n                        color={color}\n                        active\n                     >\n                        <StepButton\n                           color={color}\n                           left={'0'}\n                           onClick={() => setCurrentStep(1)}\n                           dashLeft={'0'}\n                           active={currentStep === 1}\n                        >\n                           {t('interview')}\n                        </StepButton>\n                     </Dash>\n\n                     <Dash\n                        left={'34%'}\n                        color={color}\n                        active={currentStep >= 2}\n                     >\n                        <StepButton\n                           color={color}\n                           left={'-35px'}\n                           onClick={() => setCurrentStep(2)}\n                           active={currentStep === 2}\n                        >\n                           {t('analysis')}\n                        </StepButton>\n                     </Dash>\n\n                     <Dash\n                        left={'64%'}\n                        color={color}\n                        active={currentStep >= 3}\n                     >\n                        <StepButton\n                           color={color}\n                           left={'-50px'}\n                           onClick={() => setCurrentStep(3)}\n                           active={currentStep === 3}\n                        >\n                           {t('briefing')}\n                        </StepButton>\n                     </Dash>\n\n                     <Dash\n                        left={'89%'}\n                        color={color}\n                        active={currentStep >= 4}\n                     >\n                        <StepButton\n                           color={color}\n                           left={'-22px'}\n                           onClick={() => setCurrentStep(4)}\n                           active={currentStep === 4}\n                        >\n                           {t('budget')}\n                        </StepButton>\n                     </Dash>\n                  </Line>\n               </LineWrapper>\n\n               <LineWrapper>\n                  <Line\n                     ref={line2}\n                  >\n                     <LineLabel\n                        left={'6%'}\n                     >\n                        {t('second_stage')}\n     